{
  "name": "tourze/json-rpc-http-direct-call-bundle",
  "description": "JsonRPC另外一种调用方式",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/uid": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/backtrace-helper": "1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/json-rpc-core": "0.0.*",
    "tourze/json-rpc-encrypt-bundle": "0.1.*",
    "tourze/json-rpc-endpoint-bundle": "0.1.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-routing-auto-loader-bundle": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCHttpDirectCallBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCHttpDirectCallBundle\\Tests\\": "tests"
    }
  }
}