{
  "name": "tourze/json-rpc-http-direct-call-bundle",
  "description": "JsonRPC另外一种调用方式",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/uid": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/json-rpc-endpoint-bundle": "0.0.*",
    "tourze/symfony-routing-auto-loader-bundle": "0.0.*",
    "psr/log": "^3|^2|^1",
    "yiisoft/json": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCHttpDirectCallBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCHttpDirectCallBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  }
}